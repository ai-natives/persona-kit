{
  "name": "career-navigator",
  "version": "1.0.0",
  "description": "Career Navigator Demo for PersonaKit",
  "private": true,
  "scripts": {
    "start": "./start.sh",
    "stop": "./stop.sh",
    "install:all": "pnpm run install:backend && pnpm run install:frontend",
    "install:backend": "cd backend && uv pip install -r requirements.txt",
    "install:frontend": "cd frontend && pnpm install",
    "clean": "rm -rf backend/__pycache__ frontend/node_modules frontend/dist",
    "test": "uv run python test_integration.py"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}